<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:id="@+id/main"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:background="@drawable/app_background">

    <TextView
        android:id="@+id/admin_top_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="92dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="93dp"
        android:text="Admin"
        android:textSize="32dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/events_view"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <View
        android:id="@+id/events_view"
        android:layout_width="382dp"
        android:layout_height="81dp"
        android:layout_marginTop="140dp"
        android:background="@drawable/rounded_corners"
        app:layout_constraintBottom_toTopOf="@+id/profiles_view"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.306" />

    <View
        android:id="@+id/profiles_view"
        android:layout_width="382dp"
        android:layout_height="81dp"
        android:layout_marginTop="256dp"
        android:background="@drawable/rounded_corners"
        app:layout_constraintBottom_toTopOf="@+id/images_view"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.113" />

    <View
        android:id="@+id/images_view"
        android:layout_width="382dp"
        android:layout_height="81dp"
        android:layout_marginTop="368dp"
        android:background="@drawable/rounded_corners"
        app:layout_constraintBottom_toTopOf="@+id/facilities_view"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.044" />


    <View

        android:id="@+id/facilities_view"
        android:layout_width="382dp"
        android:layout_height="81dp"
        android:layout_marginTop="480dp"
        android:background="@drawable/rounded_corners"
        app:layout_constraintBottom_toTopOf="@+id/data_view"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <View
        android:id="@+id/data_view"
        android:layout_width="382dp"
        android:layout_height="81dp"
        android:layout_marginTop="588dp"
        android:background="@drawable/rounded_corners"
        app:layout_constraintBottom_toTopOf="@+id/settings"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.482"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/side_arrow_button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@null"
        app:layout_constraintBottom_toBottomOf="@+id/events_view"
        app:layout_constraintEnd_toEndOf="@+id/events_view"
        app:layout_constraintHorizontal_bias="0.919"
        app:layout_constraintStart_toEndOf="@+id/events_text"
        app:layout_constraintTop_toTopOf="@+id/events_view"
        app:layout_constraintVertical_bias="0.491"
        app:srcCompat="@drawable/arrow" />

    <ImageButton
        android:id="@+id/side_arrow_button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="368dp"
        android:background="@null"
        app:layout_constraintBottom_toBottomOf="@+id/profiles_view"
        app:layout_constraintEnd_toEndOf="@+id/profiles_view"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/profiles_view"
        app:layout_constraintVertical_bias="0.508"
        app:srcCompat="@drawable/arrow" />

    <ImageButton
        android:id="@+id/side_arrow_button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="200dp"
        android:background="@null"
        app:layout_constraintBottom_toBottomOf="@+id/images_view"
        app:layout_constraintStart_toEndOf="@+id/images_text"
        app:layout_constraintTop_toTopOf="@+id/images_view"
        app:layout_constraintVertical_bias="0.426"
        app:srcCompat="@drawable/arrow" />

    <ImageButton
        android:id="@+id/side_arrow_button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@null"
        app:layout_constraintBottom_toBottomOf="@+id/facilities_view"
        app:layout_constraintEnd_toEndOf="@+id/facilities_view"
        app:layout_constraintHorizontal_bias="0.897"
        app:layout_constraintStart_toEndOf="@+id/facilities_text"
        app:layout_constraintTop_toTopOf="@+id/facilities_view"
        app:layout_constraintVertical_bias="0.442"
        app:srcCompat="@drawable/arrow" />


    <ImageButton
        android:id="@+id/side_arrow_button5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="368dp"
        android:background="@null"
        app:layout_constraintBottom_toBottomOf="@+id/data_view"
        app:layout_constraintEnd_toEndOf="@+id/data_view"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/data_view"
        app:layout_constraintVertical_bias="0.508"
        app:srcCompat="@drawable/arrow" />

    <TextView
        android:id="@+id/events_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="92dp"
        android:text="Events"
        android:textSize="22dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/events_view"
        app:layout_constraintEnd_toEndOf="@+id/events_view"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/events_view" />

    <TextView
        android:id="@+id/profiles_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Profiles"
        android:textSize="22dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/profiles_view"
        app:layout_constraintHorizontal_bias="0.07"

        app:layout_constraintEnd_toStartOf="@+id/side_arrow_button2"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toTopOf="@+id/profiles_view" />

    <TextView
        android:id="@+id/images_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="92dp"
        android:layout_marginEnd="93dp"
        android:text="Images"
        android:textSize="22dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/images_view"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/images_view"
        app:layout_constraintVertical_bias="0.49" />

    <TextView
        android:id="@+id/facilities_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="93dp"
        android:text="Facilities"
        android:textSize="22dp"
        android:textStyle="bold"
        app:layout_constraintHorizontal_bias="0.07"

        app:layout_constraintBottom_toBottomOf="@+id/facilities_view"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:layout_constraintTop_toTopOf="@+id/facilities_view" />

    <TextView
        android:id="@+id/data_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Hashed QR Data"
        android:textSize="22dp"
        android:textStyle="bold"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintBottom_toBottomOf="@+id/data_view"
        app:layout_constraintEnd_toStartOf="@+id/side_arrow_button5"
        app:layout_constraintStart_toEndOf="@+id/imageView5"
        app:layout_constraintTop_toTopOf="@+id/data_view"/>

    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/events_view"

        app:layout_constraintEnd_toStartOf="@+id/events_text"
        app:layout_constraintStart_toStartOf="@+id/events_view"
        app:layout_constraintTop_toTopOf="@+id/events_view"
        app:srcCompat="@drawable/calendar_image" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        app:layout_constraintBottom_toBottomOf="@+id/profiles_view"
        app:layout_constraintStart_toStartOf="@+id/profiles_view"
        app:layout_constraintTop_toTopOf="@+id/profiles_view"
        app:srcCompat="@drawable/calendar_image" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/images_view"
        app:layout_constraintEnd_toStartOf="@+id/images_text"
        app:layout_constraintStart_toStartOf="@+id/images_view"
        app:layout_constraintTop_toTopOf="@+id/images_view"
        app:srcCompat="@drawable/calendar_image" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"

        app:layout_constraintBottom_toBottomOf="@+id/facilities_view"
        app:layout_constraintStart_toStartOf="@+id/facilities_view"
        app:layout_constraintTop_toTopOf="@+id/facilities_view"
        app:layout_constraintVertical_bias="0.428"
        app:srcCompat="@drawable/calendar_image" />

    <ImageView

        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"

        app:layout_constraintBottom_toBottomOf="@+id/data_view"
        app:layout_constraintStart_toStartOf="@+id/data_view"
        app:layout_constraintTop_toTopOf="@+id/data_view"
        app:srcCompat="@drawable/calendar_image" />

    <com.google.android.material.bottomnavigation.BottomNavigationView
        android:id="@+id/bottom_navigation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/rounded_bottom_nav"
        android:fadeScrollbars="false"
        android:paddingTop="2dp"
        app:backgroundTint="#EEFFFFFF"
        app:itemIconTint="@color/black"
        app:itemTextColor="@color/black"
        app:labelVisibilityMode="labeled"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:menu="@menu/bottom_nav_menu" />






</androidx.constraintlayout.widget.ConstraintLayout>